{
  "name": "@youri-kane/socket.io-router",
  "version": "0.0.2",
  "description": "Socket.io router like api",
  "author": {
    "email": "pailloux.youri@gmail.com",
    "name": "Youri",
    "url": "https://youri-pailloux.xyz"
  },
  "main": "index.html",
  "engines": {
    "node": "^12.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "clean-build": "npm run clean && npm run build",
    "prepublishOnly": "npm run clean && npm run build && npm version patch",
    "start:example": "npm run prepublishOnly && nodemon --watch \"./\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node ./example/index.ts\""
  },
  "dependencies": {
    "path-to-regexp": "^6.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "socket.io": "^4.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "socket.io": "^4.0.1"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "typescript",
    "javascript"
  ],
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "node_modules/*"
    ],
    "delay": "1000"
  }
}
